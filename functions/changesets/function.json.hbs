{
  "description": "Publish OSM changesets into Kinesis as JSON",
  "runtime": "nodejs6.10",
  "hooks": {
    "build": "npm install"
  },
  "environment": {
    "S3_BUCKET": "{{S3_CHECKPOINT_BUCKET}}",
    "S3_CHECKPOINT_KEY": "changesets_sequence.txt",
    "STREAM_NAME": "{{CHANGESET_STREAM_NAME}}"
  }
}
