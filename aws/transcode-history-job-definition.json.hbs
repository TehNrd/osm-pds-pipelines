{
    "jobDefinitionName": "transcode-history",
    "type": "container",
    "retryStrategy": {
        "attempts": 2
    },
    "containerProperties": {
        "image": "quay.io/mojodna/osm-pds-pipelines",
        "vcpus": 1,
        "memory": 15000,
        "command": [
            "transcode",
            "--history",
            "Ref::src",
            "Ref::dst"
        ],
        "jobRoleArn": "arn:aws:iam::{{AWS_ACCOUNT_ID}}:role/osmTranscoder"
    }
}
