{
    "jobDefinitionName": "transcode-planet",
    "type": "container",
    "containerProperties": {
        "image": "quay.io/mojodna/osm-pds-pipelines",
        "vcpus": 1,
        "memory": 8000,
        "command": [
            "transcode",
            "Ref::src",
            "Ref::dst"
        ],
        "jobRoleArn": "arn:aws:iam::{{AWS_ACCOUNT_ID}}:role/osmTranscoder"
    }
}
